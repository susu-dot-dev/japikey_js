{
  "name": "@japikey/japikey",
  "version": "0.1.0",
  "description": "Library to create API keys powered by JWT and JWKS",
  "engines": {
    "node": ">=22.5.0"
  },
  "keywords": [
    "apikey",
    "jwt",
    "jwks"
  ],
  "homepage": "https://github.com/susu-dot-dev/japikey_js#readme",
  "bugs": {
    "url": "https://github.com/susu-dot-dev/japikey_js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/susu-dot-dev/japikey_js.git"
  },
  "license": "MIT",
  "author": "Anil Kulkarni",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^14.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@japikey/shared": "0.1.0",
    "jose": "^6.0.12",
    "uuid": "^11.1.0"
  }
}
